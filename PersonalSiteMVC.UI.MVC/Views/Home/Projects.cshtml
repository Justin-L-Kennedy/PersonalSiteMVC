@section scripts {
    <script>
        $(document).ready(function () {
            $('#showCurtainCall').click(function () {
                var url = $('#curtainCallModal').data('url');

                $.get(url, function (data) {
                    $('#curtainCallContainer').html(data);

                    $('#curtainCallModal').modal('show');
                });
            });
        });
    </script>
    }