<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <meta name="description" content="Justin L Kennedy is a full stack web developer utilizing Visual Studio to code HTML, CSS, JavaScript, and C#, and SSMS for SQL database management." />
    <meta name="keywords" content="Justin L Kennedy, front end, back end, full stack, developer, Visual Studio, HTML, CSS, JavaScript, JS, C#, SQL" />
    <meta name="author" content="Justin L Kennedy" />
    <title>@ViewBag.Title | Justin L Kennedy</title>

    <!-- Favicons -->
    <link href="~/Content/assets/img/favicon-16x16.png" rel="icon" type="image/png" sizes="16x16">
    <link href="~/Content/assets/img/favicon-32x32.png" rel="icon" type="image/png" sizes="32x32">
    <link href="~/Content/assets/img/apple-touch-icon.png" rel="apple-touch-icon" type="image/png" sizes="180x180">
    <link href="~/Content/assets/img/android-chrome-192x192.png" rel="icon" type="image/png" sizes="192x192">
    <link href="~/Content/assets/img/android-chrome-512x512.png" rel="icon" type="image/png" sizes="512x512">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    @*<!-- Vendor CSS Files -->
    <link href="~/Content/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/venobox/venobox.css" rel="stylesheet">
    <link href="~/Content/assets/vendor/aos/aos.css" rel="stylesheet">
    <!-- Template Main CSS File -->
    <link href="~/Content/assets/css/style.css" rel="stylesheet">*@

    @Styles.Render("~/Content/css")
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top">
        <div class="container-fluid d-flex justify-content-between align-items-center">

            <h1 class="logo"><a href="@Url.Action("Index", "Home")">Justin L Kennedy</a></h1>
            <!-- Uncomment below if you prefer to use an image logo -->
            <!--<a href="index.html" class="logo"><img src="assets/img/logo.png" alt="" class="img-fluid"></a>-->

            <nav class="nav-menu d-none d-lg-block">
                @{
                    var currentController = ViewContext.RouteData.Values["controller"] ?? "Home";
                    var currentAction = ViewContext.RouteData.Values["action"] ?? "Index";
                    var currentRoute = (currentController + "-" + currentAction).ToLower();
                }
                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Home", null, new { @class = currentRoute == "home-index" ? "active" : "" })</li>
                    <li>@Html.ActionLink("About", "About", "Home", null, new { @class = currentRoute == "home-about" ? "active" : "" })</li>
                    <li>@Html.ActionLink("Résumé", "Resume", "Home", null, new { @class = currentRoute == "home-resume" ? "active" : "" })</a></li>
                    <li>@Html.ActionLink("Projects", "Projects", "Home", null, new { @class = currentRoute == "home-projects" ? "active" : "" })</li>
                    <li>@Html.ActionLink("Links", "Links", "Home", null, new { @class = currentRoute == "home-links" ? "active" : "" })</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home", null, new { @class = currentRoute == "home-contact" ? "active" : "" })</li>
                </ul>
                <div class="nav-social-links">
                    @*<a href="#" class="twitter"><i class="icofont-twitter"></i></a>*@
                    <a href="https://www.facebook.com/Justin.Lloyd.Kennedy" target="_blank" class="facebook"><i class="icofont-facebook"></i></a>
                    @*<a href="#" class="instagram"><i class="icofont-instagram"></i></a>*@
                    <a href="https://github.com/Justin-L-Kennedy" target="_blank" class="github"><i class="icofont-github"></i></a>
                    <a href="https://www.linkedin.com/in/JustinLKennedy" target="_blank" class="linkedin"><i class="icofont-linkedin"></i></a>
                </div>
            </nav><!-- .nav-menu -->

            <div class="header-social-links d-none d-lg-block">
                @*<a href="#" class="twitter"><i class="icofont-twitter"></i></a>*@
                <a href="https://www.facebook.com/Justin.Lloyd.Kennedy" target="_blank" class="facebook"><i class="icofont-facebook"></i></a>
                @*<a href="#" class="instagram"><i class="icofont-instagram"></i></a>*@
                <a href="https://github.com/Justin-L-Kennedy" target="_blank" class="github"><i class="icofont-github"></i></a>
                <a href="https://www.linkedin.com/in/JustinLKennedy" target="_blank" class="linkedin"><i class="icofont-linkedin"></i></a>
            </div>

        </div>

    </header><!-- End Header -->

    @RenderBody()

    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="container">
            <div class="copyright">
                &copy; Copyright 2020 <strong><span>Justin L Kennedy</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/kelly-free-bootstrap-cv-resume-html-template/ -->
                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
            </div>
        </div>
    </footer><!-- End  Footer -->

    <div id="preloader"></div>
    <a href="#" class="back-to-top"><i class="bx bx-up-arrow-alt"></i></a>

    @*<!-- Vendor JS Files -->
    <script src="~/Content/assets/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/Content/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/Content/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Content/assets/vendor/counterup/counterup.min.js"></script>
    <script src="~/Content/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/Content/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Content/assets/vendor/venobox/venobox.min.js"></script>
    <script src="~/Content/assets/vendor/aos/aos.js"></script>
    <!-- Template Main JS File -->
    <script src="~/Content/assets/js/main.js"></script>*@

    @Scripts.Render("~/bundles/template")

    @RenderSection("scripts", false)

</body>

</html>